{
  "name": "metascraper-lambda",
  "version": "1.0.22",
  "repository": "https://github.com/activitree/metascraper-lambda",
  "description": "An AWS deployable Lambda + API Gateway metadata scraper",
  "main": "app.js",
  "scripts": {
    "deploy": "claudia create --region eu-west-2 --api-module app",
    "release": "claudia set-version --use-local-dependencies --version production",
    "update": "claudia update --config claudia.json",
    "destroy": "claudia destroy"
  },
  "author": "Activitree",
  "license": "MIT",
  "dependencies": {
    "claudia": "^5.13.0",
    "claudia-api-builder": "^4.1.2",
    "got": "^11.8.2",
    "metascraper": "^5.21.3",
    "metascraper-author": "^5.21.3",
    "metascraper-clearbit": "^5.21.3",
    "metascraper-date": "^5.21.3",
    "metascraper-description": "^5.21.3",
    "metascraper-image": "^5.21.3",
    "metascraper-logo": "^5.21.3",
    "metascraper-publisher": "^5.21.3",
    "metascraper-soundcloud": "^5.21.3",
    "metascraper-title": "^5.21.3",
    "metascraper-url": "^5.21.3",
    "metascraper-video": "^5.21.3",
    "metascraper-youtube": "^5.21.3",
    "tslib": "^2.1.0"
  },
  "files": [
    "app.js"
  ]
}
